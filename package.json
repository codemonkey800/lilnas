{
  "name": "lilnas",
  "version": "0.2.0",
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "build": "turbo run build",
    "clean": "rm -rf .turbo/{cache,cookies} && pnpm run -r clean",
    "clean:modules": "rm -rf node_modules packages/*/node_modules",
    "lint": "pnpm run -r lint",
    "lint:fix": "pnpm run -r lint:fix",
    "test": "pnpm run --recursive --if-present test",
    "test:watch": "pnpm run --recursive --if-present test:watch",
    "type-check": "pnpm run -r type-check"
  },
  "devDependencies": {
    "@lilnas/eslint": "workspace:*",
    "@lilnas/prettier": "workspace:*",
    "@nestjs/cli": "11.0.5",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.12",
    "@tailwindcss/postcss": "4.0.15",
    "@types/express": "5.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.14",
    "@types/json-schema": "7.0.15",
    "@types/lodash": "4.17.16",
    "@types/mime-types": "^2.1.4",
    "@types/node": "22.13.11",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/source-map-support": "0.5.10",
    "@types/uuid": "10.0.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.23.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "nodemon": "3.1.9",
    "npm-run-all": "4.1.5",
    "pino-pretty": "13.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "4.0.15",
    "ts-jest": "29.2.6",
    "tsx": "4.19.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2",
    "zx": "8.4.1"
  }
}
