{
  "name": "lilnas",
  "version": "0.2.0",
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "build": "turbo run build",
    "clean": "rm -rf .turbo/{cache,cookies} && pnpm run -r clean",
    "clean:modules": "rm -rf node_modules packages/*/node_modules",
    "lint": "pnpm run -r lint",
    "lint:fix": "pnpm run -r lint:fix",
    "test": "pnpm run --recursive --if-present test",
    "test:watch": "pnpm run --recursive --if-present test:watch",
    "type-check": "pnpm run -r type-check"
  },
  "devDependencies": {
    "@lilnas/eslint": "workspace:*",
    "@lilnas/prettier": "workspace:*",
    "@nestjs/cli": "11.0.10",
    "@nestjs/testing": "11.1.6",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.5",
    "@tailwindcss/postcss": "4.1.14",
    "@types/express": "5.0.3",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "30.0.0",
    "@types/json-schema": "7.0.15",
    "@types/lodash": "4.17.20",
    "@types/mdast": "4.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@types/semver": "7.7.1",
    "@types/source-map-support": "0.5.10",
    "@types/uuid": "10.0.0",
    "@types/wicg-file-system-access": "2023.10.7",
    "@vitejs/plugin-react-swc": "4.1.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.37.0",
    "jest": "30.2.0",
    "jest-environment-node": "30.2.0",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "pino": "^10.0.0",
    "pino-pretty": "13.1.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.14",
    "ts-jest": "29.4.5",
    "tsx": "4.20.6",
    "turbo": "^2.5.8",
    "typescript": "5.9.3",
