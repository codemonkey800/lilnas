{
  "name": "me-token-tracker",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.6.5",
  "prettier": "prettier-lilnas",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "nest build",
    "clean": "rm -rf dist/",
    "dev": "nest start -w | pino-pretty",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "NODE_ENV=production node dist/main",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "10.4.4",
    "@nestjs/core": "10.4.4",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "10.4.4",
    "@nestjs/schedule": "^4.1.1",
    "axios": "1.7.7",
    "coingecko-api-v3": "^0.0.29",
    "dedent": "1.5.3",
    "discord.js": "14.16.3",
    "dotenv": "16.4.5",
    "http-server": "^14.1.1",
    "necord": "6.8.6",
    "nestjs-pino": "^4.1.0",
    "npm-run-all": "4.1.5",
    "pino-http": "^10.3.0",
    "reflect-metadata": "0.2.2",
    "source-map-support": "0.5.21",
    "ts-pattern": "^5.5.0"
  }
}
