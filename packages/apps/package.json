{
  "name": "apps",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "prettier": "prettier-config-lilnas",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next/ *.tsbuildinfo",
    "dev": "next dev --turbopack -p 8080",
    "dev:prod": "run-s -l build start",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "NODE_ENV=production node .next/standalone/server",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.0.3",
    "node-docker-api": "^1.1.22",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
