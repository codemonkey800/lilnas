{
  "name": "@lilnas/download",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.13.1",
  "prettier": "@lilnas/prettier",
  "files": [
    "./.next",
    "./dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:backend": "nest build -b swc --type-check",
    "build:frontend": "next build",
    "clean": "rm -rf .next/ dist/ .turbo/",
    "deploy": "fly deploy",
    "dev": "run-p 'dev:*'",
    "dev:backend": "nest start -w -b swc | pino-pretty",
    "dev:frontend": "next dev -p 8080",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "run-p start:*",
    "start:backend": "node dist/main",
    "start:frontend": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:ytdlp-update": "jest --testPathPattern=ytdlp-update",
    "test:ytdlp-update:docker": "docker build -f src/ytdlp-update/__tests__/Dockerfile.test -t ytdlp-update-test . && docker run --rm ytdlp-update-test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@lilnas/utils": "workspace:*",
    "@mui/icons-material": "6.4.8",
    "@mui/material": "6.4.8",
    "@mui/material-nextjs": "6.4.3",
    "@nestjs/common": "11.0.12",
    "@nestjs/core": "11.0.12",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/schedule": "4.1.1",
    "@react-input/mask": "^2.0.4",
    "axios": "1.8.4",
    "cheerio": "1.0.0",
    "chokidar": "4.0.3",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "dedent": "1.5.3",
    "dotenv": "16.4.7",
    "fs-extra": "11.3.0",
    "http-server": "14.1.1",
    "jotai": "2.12.2",
    "lodash": "4.17.21",
    "mime-types": "^2.1.35",
    "minio": "8.0.5",
    "nanoid": "5.1.5",
    "nestjs-minio": "2.6.2",
    "nestjs-pino": "4.4.0",
    "nestjs-zod": "^4.3.1",
    "next": "15.2.3",
    "npm-run-all": "4.1.5",
    "pino-http": "10.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "0.2.2",
    "semver": "7.6.3",
    "source-map-support": "0.5.21",
    "tailwind-merge": "3.0.2",
    "ts-pattern": "5.6.2",
    "uuid": "11.1.0",
    "zod": "3.24.2"
  },
  "devDependencies": {}
}
