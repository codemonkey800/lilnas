{
  "name": "@lilnas/download",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.18.2",
  "prettier": "@lilnas/prettier",
  "files": [
    "./.next",
    "./dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:backend": "nest build -b swc --type-check",
    "build:frontend": "next build",
    "clean": "rm -rf .next/ dist/ .turbo/",
    "deploy": "fly deploy",
    "dev": "run-p 'dev:*'",
    "dev:backend": "nest start -w -b swc | pino-pretty",
    "dev:frontend": "next dev -p 8080",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "run-p start:*",
    "start:backend": "node dist/main",
    "start:frontend": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ytdlp-update": "docker build -f src/ytdlp-update/__tests__/Dockerfile.test -t ytdlp-update-test . && docker run --rm ytdlp-update-test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@lilnas/utils": "workspace:*",
    "@mui/icons-material": "7.3.4",
    "@mui/material": "7.3.4",
    "@mui/material-nextjs": "7.3.3",
    "@nestjs/common": "11.1.6",
    "@nestjs/core": "11.1.6",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/schedule": "6.0.1",
    "@react-input/mask": "^2.0.4",
    "axios": "1.12.2",
    "cheerio": "1.1.2",
    "chokidar": "4.0.3",
    "clsx": "2.1.1",
    "dayjs": "1.11.18",
    "dedent": "1.7.0",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.2",
    "http-server": "14.1.1",
    "jotai": "2.15.0",
    "lodash": "4.17.21",
    "mime-types": "^3.0.1",
    "minio": "8.0.6",
    "nanoid": "5.1.6",
    "nestjs-minio": "2.6.2",
    "nestjs-pino": "4.4.1",
    "nestjs-zod": "^5.0.1",
    "next": "15.5.4",
    "pino-http": "11.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "0.2.2",
    "semver": "7.7.3",
    "source-map-support": "0.5.21",
    "tailwind-merge": "3.3.1",
    "ts-pattern": "5.8.0",
    "uuid": "13.0.0",
    "zod": "4.1.12"
  }
}
