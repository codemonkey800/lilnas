{
  "name": "@lilnas/tdr-bot",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.18.2",
  "prettier": "@lilnas/prettier",
  "files": [
    "./.next",
    "./dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:backend": "nest build -b swc --type-check",
    "build:frontend": "next build",
    "clean": "rm -rf .next/ dist/ .turbo/",
    "deploy": "fly deploy",
    "dev": "run-p 'dev:!(graph-test)'",
    "dev:backend": "nest start -w -b swc | pino-pretty",
    "dev:frontend": "next dev -p 8080 --turbopack",
    "dev:graph-test": "GRAPH_TEST=true pnpm dev:backend",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "run-p start:*",
    "start:backend": "node dist/main",
    "start:frontend": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@langchain/community": "0.3.57",
    "@langchain/core": "0.3.78",
    "@langchain/langgraph": "0.4.9",
    "@langchain/openai": "0.6.15",
    "@libsql/client": "0.15.15",
    "@lilnas/utils": "workspace:*",
    "@nestjs/common": "11.1.6",
    "@nestjs/core": "11.1.6",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/schedule": "6.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "5.90.2",
    "@tavily/core": "0.5.12",
    "async-mutex": "^0.5.0",
    "axios": "1.12.2",
    "cheerio": "1.1.2",
    "chokidar": "4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "dayjs": "1.11.18",
    "dedent": "1.7.0",
    "discord.js": "14.23.2",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.2",
    "geist": "^1.5.1",
    "http-server": "14.1.1",
    "jotai": "2.15.0",
    "lodash": "4.17.21",
    "lru-cache": "11.2.2",
    "lucide-react": "^0.546.0",
    "minio": "8.0.6",
    "motion": "^11.18.0",
    "nanoid": "5.1.6",
    "necord": "6.11.1",
    "nestjs-minio": "2.6.2",
    "nestjs-pino": "4.4.1",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "node-docker-api": "1.1.22",
    "node-schedule": "2.1.1",
    "npm-run-all": "4.1.5",
    "openai": "6.3.0",
    "pino-http": "11.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "0.2.2",
    "remark": "15.0.1",
    "sonner": "^2.0.7",
    "source-map-support": "0.5.21",
    "tailwind-merge": "3.3.1",
    "ts-pattern": "5.8.0",
    "unist-util-visit": "5.0.0",
    "uuid": "13.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "tailwindcss-animate": "^1.0.7"
  }
}
