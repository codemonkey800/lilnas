{
  "name": "@lilnas/tdr-bot",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.6.5",
  "prettier": "@lilnas/prettier",
  "files": [
    "./.next",
    "./dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:backend": "nest build -b swc --type-check",
    "build:frontend": "next build",
    "clean": "rm -rf .next/ dist/ .turbo/",
    "deploy": "fly deploy",
    "dev": "run-p 'dev:!(graph-test)'",
    "dev:backend": "nest start -w -b swc | pino-pretty",
    "dev:frontend": "next dev -p 8080",
    "dev:graph-test": "GRAPH_TEST=true pnpm dev:backend",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "run-p start:*",
    "start:backend": "node dist/main",
    "start:frontend": "node .next/standalone/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@langchain/community": "0.3.36",
    "@langchain/core": "0.3.42",
    "@langchain/langgraph": "0.2.57",
    "@langchain/openai": "0.4.7",
    "@libsql/client": "0.15.0",
    "@lilnas/utils": "workspace:*",
    "@mui/icons-material": "6.4.8",
    "@mui/material": "6.4.8",
    "@mui/material-nextjs": "6.4.3",
    "@nestjs/common": "11.0.12",
    "@nestjs/core": "11.0.12",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/schedule": "5.0.1",
    "@tanstack/react-query": "5.69.0",
    "@tavily/core": "0.3.3",
    "axios": "1.8.4",
    "cheerio": "1.0.0",
    "chokidar": "4.0.3",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "dedent": "1.5.3",
    "discord.js": "14.18.0",
    "dotenv": "16.4.7",
    "fs-extra": "11.3.0",
    "http-server": "14.1.1",
    "jotai": "2.12.2",
    "lodash": "4.17.21",
    "lru-cache": "11.0.2",
    "minio": "8.0.5",
    "nanoid": "5.1.5",
    "necord": "6.8.8",
    "nestjs-minio": "2.6.2",
    "nestjs-pino": "4.4.0",
    "next": "15.2.3",
    "node-docker-api": "1.1.22",
    "node-schedule": "2.1.1",
    "npm-run-all": "4.1.5",
    "openai": "4.89.0",
    "pino-http": "10.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "0.2.2",
    "remark": "15.0.1",
    "source-map-support": "0.5.21",
    "tailwind-merge": "3.0.2",
    "ts-pattern": "5.6.2",
    "unist-util-visit": "5.0.0",
    "uuid": "11.1.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/mdast": "4.0.4",
    "@nestjs/testing": "11.0.12"
  }
}
