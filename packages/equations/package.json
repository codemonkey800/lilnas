{
  "name": "equations",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.5.2",
  "prettier": "prettier-lilnas",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "nest build",
    "clean": "rm -rf dist/",
    "dev": "nest start -w | pino-pretty",
    "dev:watch": "nodemon --watch src --exec 'pnpm dev'",
    "lint": "run-p -l 'lint:!(fix)'",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c src",
    "lint:fix": "run-p -l lint:*:fix",
    "lint:eslint:fix": "eslint --fix src",
    "lint:prettier:fix": "prettier -w src",
    "start": "NODE_ENV=production node dist/main",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "11.0.10",
    "@nestjs/core": "11.0.10",
    "@nestjs/platform-express": "11.0.10",
    "dotenv": "16.4.7",
    "fs-extra": "^11.3.0",
    "necord": "6.8.7",
    "nestjs-pino": "^4.3.0",
    "npm-run-all": "4.1.5",
    "pino-http": "^10.4.0",
    "reflect-metadata": "0.2.2",
    "source-map-support": "0.5.21",
    "ts-pattern": "^5.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.4",
    "@types/express": "5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "4.17.15",
    "@types/node-schedule": "2.1.7",
    "@types/source-map-support": "0.5.10",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint-config-lilnas": "workspace:*",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier-lilnas": "workspace:*",
    "tsx": "4.19.3"
  }
}
