{
  "name": "tdr-bot",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "run-p build:*",
    "build:backend": "nest build",
    "build:frontend": "next build",
    "clean": "rm -rf .next dist",
    "deploy": "fly deploy",
    "dev": "run-p 'dev:!(graph-test)'",
    "dev:backend": "nest start -w | pino-pretty",
    "dev:frontend": "next dev -p 8080",
    "dev:graph-test": "GRAPH_TEST=true pnpm dev:backend",
    "lint": "eslint .",
    "start": "run-p start:*",
    "start:backend": "NODE_ENV=production node dist/main",
    "start:frontend": "NODE_ENV=production node server",
    "type-check": "tsc --noEmit"
  },
